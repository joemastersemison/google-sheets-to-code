{
  "name": "google-sheets-to-code",
  "version": "1.0.0",
  "description": "Convert Google Sheets logic to TypeScript or Python code",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test": "NODE_OPTIONS=\"--experimental-vm-modules\" jest",
    "test:watch": "NODE_OPTIONS=\"--experimental-vm-modules\" jest --watch",
    "test:coverage": "NODE_OPTIONS=\"--experimental-vm-modules\" jest --coverage",
    "cli": "tsx src/cli/index.ts",
    "lint": "biome lint src/",
    "lint:fix": "biome lint --write src/",
    "format": "biome format src/",
    "format:fix": "biome format --write src/",
    "check": "biome check src/",
    "check:fix": "biome check --write src/",
    "typecheck": "tsc --noEmit",
    "prepare": "husky"
  },
  "keywords": [
    "google-sheets",
    "code-generation",
    "typescript",
    "python"
  ],
  "author": "Joe Emison",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "^2.2.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.11.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.11",
    "ts-jest": "^29.2.5",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@google-cloud/local-auth": "^3.0.1",
    "chevrotain": "^11.0.3",
    "commander": "^11.1.0",
    "googleapis": "^144.0.0"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx,json,md}": [
      "biome check --write --no-errors-on-unmatched"
    ]
  },
  "overrides": {
    "color-convert": "^2.0.1",
    "debug": "^4.4.1"
  }
}
